<?xml version='1.0' encoding='UTF-8'?>
<!DOCTYPE beans PUBLIC '-//SPRING//DTD BEAN//EN' 'http://www.springframework.org/dtd/spring-beans.dtd'>
<beans>
    <bean id="${project.artifactId}_bootstrapSites" parent="spacesStoreImporter">
        <property name="useExistingStore" value="true"/>
        <property name="transactionService" ref="transactionService"/>
        <property name="bootstrapViews">
            <list>
                <props>
                    <prop key="path">/app:company_home/st:sites</prop>
                    <prop key="uuidBinding">UPDATE_EXISTING</prop>
                    <prop key="location">alfresco/module/${project.artifactId}/bootstrap/sites/edit-online-default-site.xml</prop>
                </props>
            </list>
        </property>
    </bean>

</beans>